<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <solution>
	 <!-- 默認不將 packages 提交到源代碼管理 -->
     <add key="disableSourceControlIntegration" value="true"/>
  </solution>
  <config>
	<!-- 指定這個目錄下默認的 packages 目錄 -->
    <add key="repositorypath" value="..\src\packages" />
  </config>
  <packageRestore>
	<!-- 默認啟用 packages 還原 -->
	<add key="enabled" value="True"/>
  </packageRestore>
</configuration>
